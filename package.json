{
	"name": "bauhaus",
	"version": "0.0.0",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*",
		"app"
	],
	"scripts": {
		"start": "yarn workspace Bauhaus run start",
		"build-insee": "yarn workspaces run build-insee",
		"test:coverage": "yarn workspaces run test:coverage",
		"test:ci": "jest",
		"build-storybook": "yarn workspaces run build-storybook",
		"pre-push": "yarn workspaces run lint && yarn test:coverage"
	},
	"resolutions": {
		"@inseefr/wilco": "0.0.5"
	},
	"devDependencies": {
		"@storybook/addon-actions": "5.3.19",
		"@storybook/addon-knobs": "5.3.19",
		"@storybook/addon-links": "5.3.19",
		"@storybook/addons": "5.3.19",
		"@storybook/react": "5.3.19",
		"@testing-library/jest-dom": "5.11.2",
		"@testing-library/react": "10.4.7",
		"@testing-library/user-event": "12.0.17",
		"@types/jest": "26.0.5",
		"babel-eslint": "^10.1.0",
		"babel-jest": "26.0.1",
		"babel-loader": "8.1.0",
		"eslint": "7.5.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-config-react-app": "5.2.1",
		"eslint-plugin-flowtype": "5.2.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jest-dom": "3.1.4",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-react": "7.20.3",
		"eslint-plugin-react-hooks": "4.0.8",
		"eslint-plugin-testing-library": "3.3.2",
		"husky": "3.0.2",
		"jest": "26.0.1",
		"jest-localstorage-mock": "2.4.2",
		"jest-sonar-reporter": "^2.0.0",
		"lcov-result-merger": "3.1.0",
		"webpack": "4.44.0"
	},
	"dependencies": {
		"prop-types": "15.7.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-redux": "7.2.0",
		"react-router-dom": "5.2.0",
		"redux": "4.0.5"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn pre-push"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	},
	"jest": {
		"transform": {
			"^.+\\.js$": "babel-jest"
		},
		"testResultsProcessor": "jest-sonar-reporter"
	}
}
